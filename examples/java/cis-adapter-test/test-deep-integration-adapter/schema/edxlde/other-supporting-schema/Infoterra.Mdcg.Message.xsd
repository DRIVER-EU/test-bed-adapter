		</xs:annotation>
		<xs:sequence>
			<xs:element name="IncidentTypeValue" type="xs:string">
				<xs:annotation>
					<xs:documentation>Type of incident, can be a string or an Mdac Value, if an Mdac value set the IncidentTypeFormat accordingly</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IncidentTypeFormat" type="IncidentTypeFormatEnum">
				<xs:annotation>
					<xs:documentation>Reference to the IncidentTypeFormat enum</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="RiskInformation">
		<xs:annotation>
			<xs:documentation>Details for a URN risk</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Nurn" type="xs:unsignedInt">
				<xs:annotation>
					<xs:documentation>The Nurn reference number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RiskCoordinates" type="Location">
				<xs:annotation>
					<xs:documentation>The location of the risk</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="RiskVersion" type="xs:unsignedInt">
				<xs:annotation>
					<xs:documentation>Version of the risk data</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EndPointRequestType">
		<xs:annotation>
			<xs:documentation>End point request message, Endpoint request enum specifies the type of request it is</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EndPointRequest" type="EndPointRequestEnum">
				<xs:annotation>
					<xs:documentation>Enum specifying the type of lookup</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="EndPoint" type="EndPointType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Used to specify the endpoint that is a basis of the request</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="BearerEndPointType">
		<xs:annotation>
			<xs:documentation>Bearer EndPoint Type</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EndPoint" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>The endpoint details, will be in a different format depending on the bearer</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Bearer" type="xs:string" nillable="true">
				<xs:annotation>
					<xs:documentation>The type of bearer</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EndPoints">
		<xs:annotation>
			<xs:documentation>An unbounded list of endpoints</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EndPoints" type="EndPointType" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="EndPointType">
		<xs:annotation>
			<xs:documentation>A complex type to hold a callsign, device type and list of endpoints</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element ref="CallSign">
				<xs:annotation>
					<xs:documentation>The CallSign</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Resource" type="CallSignCapabilityEnum">
				<xs:annotation>
					<xs:documentation>The device type</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="CallSignEndPoints" type="CallSignEndPointsType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>The unbounded list of endpoints for this callsign</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="CallSignEndPointsType">
		<xs:annotation>
			<xs:documentation>A complex type to an endpoint and a datetime field which is used to specify when it was last updated</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="EndPoint" type="BearerEndPointType">
				<xs:annotation>
					<xs:documentation>The endpoint</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Updated" type="xs:dateTime">
				<xs:annotation>
					<xs:documentation>DateTime of when the endpoint details were last updated</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="PointFeatureType">
		<xs:annotation>
			<xs:documentation>Complex type to hold a point feature</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Name" type="xs:string">
				<xs:annotation>
					<xs:documentation>Name of the point feature</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Text" type="xs:string">
				<xs:annotation>
					<xs:documentation>Associated text</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Location" type="Location">
				<xs:annotation>
					<xs:documentation>Location of the point feature</xs:documentation>
				</xs:annotation>
