		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ResponseRequestType">
		<xs:annotation>
			<xs:documentation>an individual response type</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="RequestID" type="xs:int">
				<xs:annotation>
					<xs:documentation>Request id is used to identify a single assistance or planned response request such as Make Pumps.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IParamID" type="xs:int" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>IParamID is used to identify an instance of a integer Parameter associated with a response request.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="IParamValue" type="xs:int" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>IParamValue is used to hold the value of the integer Parameter associated with a response request.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SParamID" type="xs:int" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>SParamID is used to identify an instance of a string Parameter associated with a response request.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SParamValue" type="xs:string" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>SParamValue is used to hold the value of the string Parameter associated with a response request.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ReasonID" type="xs:int" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ReasonID is used to identify an instance of a Reason associated with a response request.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="ResponseLocation" type="Location" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ResponseLocation is used to identify the location associated with a response request.</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IntegerParametersType">
		<xs:annotation>
			<xs:documentation>Unbounded list of parameters, used from the status message</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Parameter" type="IntegerParameterType" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>Unbounded list of parameters</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="IntegerParameterType">
		<xs:annotation>
			<xs:documentation>Complex type to hold parameter details, used within a status message</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Value" type="xs:int" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Value returned from VMDS</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="DisplayName" type="xs:string" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Display name the parameter is related to</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SpecialValueID" type="xs:int" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The ID the value is related to</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="EndPointRequestEnum">
		<xs:annotation>
			<xs:documentation>The various request that can be sent to the Master Directory</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="Full">
				<xs:annotation>
					<xs:documentation>Request for a full list of all known clients</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="CallSign">
				<xs:annotation>
					<xs:documentation>Request for information matching the specified callsign</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="EndPoint">
				<xs:annotation>
					<xs:documentation>Request for information matching the specified endpoint</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="InterruptRequestEnum">
		<xs:annotation>
			<xs:documentation>A list of reasons why an interupt could be requested</xs:documentation>
			<!-- Low quantity enum - to store as multi-bit? Currently could be stored as 2 bits. -->
		</xs:annotation>
		<xs:restriction base="xs:NMTOKEN">
			<xs:enumeration value="Speak">
				<xs:annotation>
					<xs:documentation>Request to Speak</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Emergency">
				<xs:annotation>
					<xs:documentation>Emergency</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="SpeakConfidential">
				<xs:annotation>
					<xs:documentation>Request to speak confidentially</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="GPSQualityEnum">
		<xs:annotation>
			<xs:documentation>A list specifying the quality of GPS signal (typically based on how many satellites were locked at time of collecting location)</xs:documentation>
			<!-- Low quantity enum - to store as multi-bit? Currently could be stored as 3 bits. -->
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Invalid">
				<xs:annotation>
					<xs:documentation>Invalid GPS Data</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Unknown">
				<xs:annotation>
					<xs:documentation>Value unknown</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="NoSignal">
				<xs:annotation>
					<xs:documentation>No signal</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Poor">
				<xs:annotation>
					<xs:documentation>Poor i.e. 3 satellites</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Good">
				<xs:annotation>
					<xs:documentation>4 or more satellites locked at time of collecting the location</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PositionalErrorEnum">
		<xs:annotation>
			<xs:documentation>A list specifying the position error of location information</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
