				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LessThan20m">
				<xs:annotation>
					<xs:documentation>Less Than 20m</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LessThan200m">
				<xs:annotation>
					<xs:documentation>Less Than 200m</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LessThan2km">
				<xs:annotation>
					<xs:documentation>Less Than 2km</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LessThan20km">
				<xs:annotation>
					<xs:documentation>Less Than 20km</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="LessThanOrEqualTo200km">
