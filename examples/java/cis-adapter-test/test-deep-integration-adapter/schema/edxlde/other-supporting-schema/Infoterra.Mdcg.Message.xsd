<xs:schema xmlns="http://www.infoterra.co.uk/Mdcg" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.infoterra.co.uk/Mdcg" elementFormDefault="qualified">
	<!-- Naming Convention -->
	<!-- Elements - UCC -->
	<!-- Attributes - LCC -->
	<!-- User Types - 'Type' Suffix -->
	<!-- Enum Types - 'Enum' Suffix -->
	<!-- If we need to integrate gml, the schema include is: 
  xmlns:gml="http://www.opengis.net/gml -->
	<!-- Using abstracts instead of choices:
  http://www.ibm.com/developerworks/webservices/library/ws-tip-xsdchoice.html
  -->
	<xs:annotation>
		<xs:documentation>These are the schema definitions for the message types - they are expressed in XML Schema Definition (XSD) format</xs:documentation>
	</xs:annotation>
	<!-- Overall Message structure -->
	<xs:element name="CCXMLMessage">
		<xs:annotation>
			<xs:documentation>Dictates the overall message structure</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="Header" type="MessageHeader">
					<xs:annotation>
						<xs:documentation>The Header is common to every message</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Body" type="MessageBody">
					<xs:annotation>
						<xs:documentation>There is always a 'body' element, but the xsi:type attribute will vary depending on the message type</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<!-- Overall Message structure for OM100 bound MLP messages-->
	<xs:element name="MlpCCXMLMessage" type="MlpCCXMLMessageType">
		<xs:annotation>
			<xs:documentation>Dictates the overall message structure for MLP messages</xs:documentation>
		</xs:annotation>
		</xs:element>
		<xs:complexType name="MlpCCXMLMessageType">
			<xs:sequence>
				<xs:element name="Header" type="MessageHeader">
					<xs:annotation>
						<xs:documentation>The Header is common to every message</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="Body" type="MessageBody">
					<xs:annotation>
						<xs:documentation>There is always a 'body' element, but the xsi:type attribute will vary depending on the message type</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:element name="ReqId" type="xs:string" nillable="true" minOccurs="0">
					<xs:annotation>
						<xs:documentation>The MLP Request ID</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>

	<!-- Message Header info below -->
	<xs:complexType name="MessageHeader">
		<xs:annotation>
			<xs:documentation>The Header is common to every message</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Source" type="xs:string">
				<xs:annotation>
					<xs:documentation>Callsign of the sender of the message</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="SourceDeviceType" type="CallSignCapabilityEnum" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The device type of the sender of the message, optional</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Targets" type="TargetType">
				<xs:annotation>
					<xs:documentation>Callsign of the destination of the message</xs:documentation>
					<xs:documentation>There is always a target (although from the VMDS it's always the same target)</xs:documentation>
					<xs:documentation>In the event of multiple targets, the message will be split by the gateway into multiple individual messages with the same body and header, except there will only be one target per message. It is up to the sending application to manage the multiple ACK/NACK messages that will return (similar to emails with multiple recipients)</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="TargetsDeviceType" type="CallSignCapabilityEnum" nillable="true" minOccurs="0">
				<xs:annotation>
					<xs:documentation>The device type of the targets of the message, optional</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Sent" type="xs:dateTime" nillable="true">
				<xs:annotation>
					<xs:documentation>The time the message was generated by the sender</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element ref="MessageID">
				<xs:annotation>
					<xs:documentation>The message identifier. This must be unique for each message from the same source</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Priority" type="PriorityEnum" default="Medium">
				<xs:annotation>
					<xs:documentation>Priority of the message. Messages of a higher priority are given precedence over those with lower priority in MDCG queues</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Topic" type="xs:string">
				<xs:annotation>
					<xs:documentation>This should be identical to the xsi:type in the MessageBody. This is a string in order to enable the header structure to remain inviolate while allowing a flexible approach to what goes in the Message Body</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="AckType" type="AckType" default="None" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Specifies the type of acknowledgement (if any), expected from the recipient of the message</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:simpleType name="UnsignedInt24Type">
		<xs:annotation>
			<xs:documentation>Max of (2^24)-1 </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:unsignedInt">
			<xs:maxExclusive value="16777216"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PriorityEnum">
		<xs:annotation>
			<xs:documentation>Message priority</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="Low">
				<xs:annotation>
					<xs:documentation>Low Priority</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="Medium">
				<xs:annotation>
					<xs:documentation>Medium Priority</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="High">
				<xs:annotation>
					<xs:documentation>High Priority (typically Mobilise Messages)</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
		</xs:restriction>
	</xs:simpleType>
	<!-- End of Message Header info -->
	<xs:complexType name="MessageBody" abstract="true">
		<xs:annotation>
			<xs:documentation> This provides a base 'class' for all the messages. It allows the MessageBody to be extended without having to update the 'choice' statement as previously required</xs:documentation>
		</xs:annotation>
	</xs:complexType>
	<!-- Message Types below -->
	<!-- These are in purely for generating sample code - you need an element before you can generate
         XML for that element type -->
	<xs:complexType name="LogonNotificationMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Sent to indicate that a client has logged on to the MDCG</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Callsign" type="xs:string">
						<xs:annotation>
							<xs:documentation>The callsign of the client that has successfully logged on</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="LogoffNotificationMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Sent to indicate that a client has logged off.  Sent to the CandC if the MDT user logs off, also sent to an MDT if the callsign is assigned to a radio via an MD update or if the callsign is used from a different MDT</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Callsign" type="xs:string">
						<xs:annotation>
							<xs:documentation>The callsign of the client that has logged off or is being logged off</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Reason" type="ReasonEnum">
						<xs:annotation>
							<xs:documentation>Reason why the MDT user has been logged off from MDCG</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HeartbeatMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Sent to indicate that the MDCG has received a heartbeat message from a client over a given bearer</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Callsign" type="xs:string">
						<xs:annotation>
							<xs:documentation>The callsign of the client that sent the heartbeat</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Bearer" type="xs:string">
						<xs:annotation>
							<xs:documentation>The bearer on which the heartbeat was received</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HealthCheckRequestMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>A message sent to a component requesting a response to indicate the component is healthy</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="HealthCheckResponseMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>A message that a component will send in response to a HealthcheckRequest message</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="IsAlive" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Boolean indicating if the component is still alive</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="AckMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Sent automatically to confirm receipt of a message. AckType in the header MUST be None</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element ref="MessageID">
						<xs:annotation>
							<xs:documentation>MessageID of the message this is acknowledging, note the Ack will have it's own MessageID in the header</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ManAckMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Sent to confirm that a user has manually acknowledged a message. AckType in the header may be 'Auto' but must NOT be 'Man'</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="AckMessage"/>
			<!--<xs:sequence>
          <xs:element maxOccurs="1" minOccurs="1" ref="MessageID">
            <xs:annotation>
              <xs:documentation>MessageID of the message this is acknowledging, note the Ack will have it's own MessageID in the header</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>-->
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ComponentStartedMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Not a FiReControl message</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Component" type="xs:string">
						<xs:annotation>
							<xs:documentation>Provides an indication that a component has started</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DemobiliseMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Outbound message which instructs the target to demobilise from the specified incident number</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="About" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Used if this message is about another callsign, if specified then VMDS will not act on the message and show as information only</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="IncidentNumber" type="xs:unsignedInt" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Incident number that the target is being demobilised from</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BatchID" type="xs:unsignedInt" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The batch number that the target is being demobilised from</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ForwardID" type="xs:unsignedInt" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The forward ID number that the target is being demobilised from</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="DutyStaffingUpdateMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Used to specify the name of the officer in charge and the number of crew on the appliance</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="OfficerInCharge" type="xs:string">
						<xs:annotation>
							<xs:documentation>The name of the officer in charge</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="NumberOfCrew" type="xs:unsignedByte">
						<xs:annotation>
							<xs:documentation>Total number of crew on the appliance</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="CrewRidingMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Used to specify the crew that are on the appliance</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Crew" type="CrewMembersType">
						<xs:annotation>
							<xs:documentation>Unbounded list of crew</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OfficerInCharge" type="xs:unsignedInt" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Relates to the employeeID in the list of crew to indicate the officer in charge</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PotentialCrewRidingMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Used to specify the potential crew that are on the appliance</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Crew" type="CrewMembersType">
						<xs:annotation>
							<xs:documentation>Unbounded list of crew</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OfficerInCharge" type="xs:unsignedInt" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Relates to the employeeID in the list of crew to indicate the officer in charge</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ActualCrewRidingMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Used to specify the actual crew that are on the appliance</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Crew" type="CrewMembersType">
						<xs:annotation>
							<xs:documentation>Unbounded list of crew</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="OfficerInCharge" type="xs:unsignedInt" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Relates to the employeeID in the list of crew to indicate the officer in charge</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EndPointRequestMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>
        Used to request the end point details for a client, or for all clients. This will be sent by the MDCG at startup or if it receives a message from an unknown client. This allows the MDCG to learn of all radio clients and covers for the event of the receipt of a message from a known radio client whose details have been lost by the MDCG.
      </xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="EndPointRequest" type="EndPointRequestEnum">
						<xs:annotation>
							<xs:documentation>The type of endpoint request</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CallSign" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The callsign of the client whose details are being requested</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndPoint" type="BearerEndPointType" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The endpoint of the client whose details are being requested</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EndPointResponseMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>The response to a xsi:EndPointRequestMessage</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element ref="MessageID">
						<xs:annotation>
							<xs:documentation>
                The ID of the xsi:EndPointRequestMessage that this response is to
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="EndPoint" type="EndPointType">
						<xs:annotation>
							<xs:documentation>
                An unbounded list of the client details requested.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="EndPointMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>An unsolicited message sent to inform interested parties of changes to client details. This can be the result of a new client logging in to the MDCG, the addition of a new bearer for an existing client in the MDCG, or the change to the details of a client in the MDCG.  Similarly any new radio clients that the MDCG needs to be informed of will be updated by the receipt of this message type by the MDCG.</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="EndPoints" type="EndPoints">
						<xs:annotation>
							<xs:documentation>
                An unbounded list of the client details.
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DeleteEndpoints" type="RemovableBoolean" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Indicate the endpoints are no longer related to the callsign</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ForwardRemoveResourceMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Message used to remove a resource from the forward commanders pool of resources</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Resources" type="ResourcesType">
						<xs:annotation>
							<xs:documentation>A list of resources to be removed from the group</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ForwardID" type="xs:unsignedInt">
						<xs:annotation>
							<xs:documentation>The ID of the forward group</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ForwardRemoveIncidentMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Message to remove incident from the forward group</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:sequence/>
					<xs:element name="IncidentID" type="xs:unsignedInt">
						<xs:annotation>
							<xs:documentation>The incident to remove from the forward group</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ForwardID" type="xs:unsignedInt">
						<xs:annotation>
							<xs:documentation>The ID of the forward group</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ForwardIncidentMobilisedMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Resource allocated by forward commander to incident</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:sequence>
						<xs:element name="IncidentID" type="xs:unsignedInt">
							<xs:annotation>
								<xs:documentation>The incident the resource should mobilise to</xs:documentation>
							</xs:annotation>
						</xs:element>
					</xs:sequence>
					<xs:element name="ForwardID" type="xs:unsignedInt">
						<xs:annotation>
							<xs:documentation>The ID of the forward group</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CallSign" type="xs:string">
						<xs:annotation>
							<xs:documentation>The resource to be allocated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="DeploymentPoint" type="Location">
						<xs:annotation>
							<xs:documentation>The deployment point for the resource to use</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="InterruptRequestMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Message sent from VMDS when requesting a voice communication, typically for an emergency reason</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="RequestType" type="xs:unsignedInt">
						<xs:annotation>
							<xs:documentation>References the VMDS MDAC store of request types</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="LocalizedStatusAcknowledgementMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Localised status acknowledgment sent by a control room application to a mobile terminal through the MDG </xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="eads_name_of_vehicle" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Name of the vehicle
              </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="eads_vehicle_ident" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Vehicle Identifier </xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="Status">
						<xs:annotation>
							<xs:documentation>Integer relating to the status</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PreviousStatus" type="xs:unsignedInt">
						<xs:annotation>
							<xs:documentation>Integer relating to the status</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="LocationAlertMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Message generated by LocationMonitor and targeted at the CandC when a timeout occurs during monitoring of ARLs from a resource</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element ref="CallSign" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>Unbounded list of callsigns that haven't sent an ARL within an expected duration</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="LogUpdateMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>This is a text message relating to a specific incident</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="LogUpdate" type="LogUpdate">
						<xs:annotation>
							<xs:documentation>References the LogUpdate type</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MakeUpMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Used to request additional resources</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="MakeUp" type="MakeUp">
						<xs:annotation>
							<xs:documentation>Main body of the makeup message</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RelayedMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>The Relay message is used to relay other messages. The Message Body contains a normal message header and another message body therefore it is able to contain a normal message in its entirety</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Header" type="MessageHeader"/>
					<xs:element name="Body" type="MessageBody"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ForwardedMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>The Forward message is used to forward other messages. The Message Body contains a normal message header and another message body therefore it is able to contain a normal message in its entirety</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Header" type="MessageHeader"/>
					<xs:element name="Body" type="MessageBody"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="IncidentInformativeMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Incident Informative Message</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Content" type="xs:string">
						<xs:annotation>
							<xs:documentation>Content of Incident Informative Message</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="MobiliseMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Mobilisation message</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Incident" type="Incident">
						<xs:annotation>
							<xs:documentation>Main body of the mobilisation message</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Reallocation" type="xs:boolean" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Informs VMDS of allocation to a new incident without first being demobilised</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BatchID" type="xs:unsignedInt" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>If set this indicates this mobilisation message is part of a batch</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ForwardID" type="xs:unsignedInt" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The forward group to which this incident is allocated</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ForwardParentIncidentNumber" type="xs:unsignedInt" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The forward parent incident number</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ForwardIncidentPriority" type="xs:unsignedByte" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The priority of this forward incident applied by CandC</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ForwardCmdrCallsign" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The callsign of the forward commander</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ForwardResources" type="ForwardResourcesType" nillable="true" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>List of forward resources</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="BatchIncidentPriority" type="xs:unsignedByte" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The priority of this incident within the batch of incidents, used to order the incidents within VMDS</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PlannedResponseRequestMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>A PlannedResponseRequestMessage is a method of requesting additional resources or equipment to an incident.</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="ResponseRequests" type="ResponseRequestType" maxOccurs="unbounded">
						<xs:annotation>
							<xs:documentation>An unbounded collection of response types</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BatchUpdateMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>This is used to remove a message from the Batch. Demobilise can't be used because that marks a message as demobilised but leaves it in the batch hence this message is the mechanism for taking an incident out of the batch queue</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="IncidentNumber" type="xs:unsignedInt"/>
					<xs:element name="BatchID" type="xs:unsignedInt"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="BatchIncidentNotificationMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>This is sent from VMDS to instruct the CandC system that they wish to be mobilised to this incident. Note that this is not a request it's an instruction</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="BatchIncidentID" type="xs:unsignedInt"/>
					<xs:element name="BatchID" type="xs:unsignedInt"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="TestMobilisationMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Used to test the lines of communication between the CandC system and VMDS</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence/>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PreAlertMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>To provide advanced notification of an incident</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Incident" type="Incident"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="PreAlertRemoveMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Remove a PreAlert incident that has previously been sent to VMDS.  If mobilised to the same incident then it will automatically be removed from the preAlert list</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element ref="IncidentNumber"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="StandbyMobilisationMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Used to send appliances to a standby location</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="GenericText" type="xs:string">
						<xs:annotation>
							<xs:documentation>Generic text about the standby mobilisation that is sent to all appliances assigned to the standby location</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Reallocation" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Set to true to reallocate an appliance to this standby location without the need to demobilise them first</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="SpecificText" type="xs:string">
						<xs:annotation>
							<xs:documentation>Specific text that is sent only to the target callsigns in the header</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="StandbyLocation" type="Location">
						<xs:annotation>
							<xs:documentation>The coordinates of the standby location</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="NackMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Negative acknowledgement, can be used to reject a message or to indicate failure to deliver a message</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element ref="MessageID">
						<xs:annotation>
							<xs:documentation>MessageID of the message the Nack relates to, NOTE the NackMessage will have it's own MessageID in the header.</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Callsign" type="xs:string">
						<xs:annotation>
							<xs:documentation>The callsign of the client that was the target of the message the Nack relates to</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Nack" type="ReasonEnum">
						<xs:annotation>
							<xs:documentation>Specifies the reason the message was Nacked</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="RateControlMessage" mixed="false">
		<xs:annotation>
			<xs:documentation> This is primarily for handheld radios. Other resources will typically have status-dependant rate control business logic onboard.</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Rate" type="RateType">
						<xs:annotation>
							<xs:documentation>Used to specify time based rate</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Distance" type="DistanceType">
						<xs:annotation>
							<xs:documentation>Used to specify distance based rate</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ReportType" type="LocationReportTypeEnum">
						<xs:annotation>
							<xs:documentation>Used to specify the location report type required</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ImmediateLocationRequestMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Request an immediate response from a device containing its location</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="ReportType" type="LocationReportTypeEnum">
						<xs:annotation>
							<xs:documentation>Used to specify the location report type required</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ResetMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Currently unused</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="Reset" type="Reset">
						<xs:annotation>
							<xs:documentation>Currently unused</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ResourceARLMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Used to provide ARL details for a resource, typically sent to VMDS to provide details of another resource</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="About" type="xs:string" nillable="true" minOccurs="0"/>
					<xs:element name="Location" type="Location">
						<xs:annotation>
							<xs:documentation>Location details of the resource</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ResourceLocationMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Typically outbound from VMDS to provide location details, rate at which these are generated are based on the content of the ARLRateMessage</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="ReportType" type="LocationReportTypeEnum" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Used to specify the location report type</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ReasonForSending" type="ReasonForSendingEnum" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The reason why the location information is being sent</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Time" type="xs:dateTime" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>DateTime the location was captured</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TimeElapsed" type="TimeElapsedType" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Approximate time since location determination</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Location" type="Location">
						<xs:annotation>
							<xs:documentation>Reference to the location type that contains the location details</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="CurrentGPSQuality" type="GPSQualityEnum" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>GPS quality as it was when data was collated from GPS device</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ReportedGPSQuality" type="GPSQualityEnum" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Last reported GPS quality</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TetrapolGPSStatus" type="xs:unsignedByte" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An optional field to return the Tetrapol GPS Status</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PositionalError" type="PositionalErrorEnum" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The maximum GPS error as defined by a LIP message</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Speed" type="VehicleSpeed" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Reported speed from the GPS device at time of collating the data</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Direction" type="AngleType" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Reported direction from the GPS device at time of collating the data</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="GMBMode" type="GMBModeEnum" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>User defined data from a GMB Short Location Report message</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="eads_tetrapol_associatedRT" type="xs:integer" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Association between external tetrapol identifier and associated RSFI</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ResourceLocationWithCallsign">
		<xs:annotation>
			<xs:documentation>As per the ResourceLocationMessage with the addition of a callsign field to identify the sender</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="Callsign" type="xs:string" nillable="false">
				<xs:annotation>
					<xs:documentation>The callsign of the location</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Location" type="ResourceLocationMessage" nillable="false">
				<xs:annotation>
					<xs:documentation>The location message</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="ResourceStatusMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Used as both a request and a response, sent from VMDS to request a status change and then sent from CandC to confirm the request to change status</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element name="About" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Used if this message is about another callsign, if specified then VMDS will not act on the message and show as information only</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element ref="Status">
						<xs:annotation>
							<xs:documentation>Integer relating to the status</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Remarks" type="xs:string" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Remarks can be sent with the request and/or response</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Parameters" type="IntegerParametersType" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Reference to the parameters that can be contained within a status message</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Location" type="Location" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>The mobile terminal location to provide a localised status</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="TetrapolGPSStatus" type="xs:unsignedByte" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>An optional field to return the Tetrapol GPS Status</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Time" type="xs:dateTime" nillable="true" minOccurs="0">
						<xs:annotation>
							<xs:documentation>Time of status</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="OperationalStatusAckMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>A specific message that is used to acknowledge a status message from a Tetrapol radio, a business level ack</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element ref="Status">
						<xs:annotation>
							<xs:documentation>Integer relating to the status</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="PreviousStatus" type="xs:unsignedByte">
						<xs:annotation>
							<xs:documentation>Integer relating to the status</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Positive" type="xs:boolean">
						<xs:annotation>
							<xs:documentation>Boolean indicating if it is a positive or negative ack</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="Cause" type="xs:unsignedByte">
						<xs:annotation>
							<xs:documentation>This is the cause of the status being nacked</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="StatusChangeNotificationMessage" mixed="false">
		<xs:annotation>
			<xs:documentation>Message sent by VMDS to confirm it has changed status.  It also contains a value indicating at which reporting rate it will be sending resource location messages while in this status</xs:documentation>
		</xs:annotation>
		<xs:complexContent mixed="false">
			<xs:extension base="MessageBody">
				<xs:sequence>
					<xs:element ref="Status">
						<xs:annotation>
							<xs:documentation>Integer relating to the status</xs:documentation>
						</xs:annotation>
					</xs:element>
					<xs:element name="ARLReportingIntervalSecs" type="xs:unsignedInt">
						<xs:annotation>
							<xs:documentation>Value indicating the reporting interval that VMDS will no be reporting resource location messages at</xs:documentation>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="ResourceStatusRequestMessage" mixed="false">
